/* Configuración del parser para gramática No.1 */
/* Sigue la gramática SLR: */
/* E´ → E */
/* E  → E + T */
/* E  → T  */
/* T  → T * F */
/* T  → F */
/* F  → ( E ) */
/* F  → id */

%token ID
%token PLUS
%token TIMES
%token LPAREN RPAREN
%token WS
/*%token TEST*/
IGNORE WS

%%



e: e PLUS t
 | t
 ;

t: t TIMES f 
 | f
 ;

f: LPAREN e RPAREN
    | ID
;
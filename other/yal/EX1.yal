let delim = [' ''\t''\n']
let ws = delim+
let letter = ['a'-'z''A'-'Z']
let digit = ['0'-'9']
let digits = digit+
let id = letter(letter|digit)*
let number = digits.(('.'.digits)?.('E'['+''-']?.digits)?)?

rule tokens = 
    ws        { WS }
  | id        { ID }
  | number    { NUMBER }
  | '+'       { PLUS }
  | '-'       { MINUS }
  | '*'       { TIMES }
  | '/'       { DIV }
  | '('       { LPAREN }
  | ')'       { RPAREN }


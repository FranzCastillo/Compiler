%%
let delim = [' ''\t''\n']
let ws = delim+

let letters = ['a'-'z''A'-'Z']
let digit = ['0'-'9']
let digits = digit+

let id = letters(letters|digits|'_')*

rule tokens =
    id      { ID }
|   '+'     { PLUS }
|   '*'     { TIMES }
|   '('     { LPAREN }
|   ')'     { RPAREN }
|   ws      { WS }



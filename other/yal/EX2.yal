%%
let delim = [' ''\t''\n']
let ws = delim+
let letter = ['a'-'z''A'-'Z']
let digit = ['0'-'9']
let digits = digit+
let id = letter(letter|digit)*

rule tokens =
    ws        { WS }
  | id        { ID }
  | '+'       { PLUS }
  | '*'       { TIMES }
  | '('       { LPAREN }
  | ')'       { RPAREN }

